# Server
PORT=3000
API_VERSION=v1
ALLOWED_ORIGINS=*
RATE_LIMIT=200

# Database
DB_PATH=./data/muci-superman.db

# Features
ENABLE_CACHING=true
ENABLE_COST_TRACKING=true

# Authentication
REQUIRE_AUTH=false
JWT_SECRET=change-this-to-a-secure-random-string
JWT_EXPIRES_IN=24h
ADMIN_USER=admin
ADMIN_PASS=change-this-password
ADMIN_API_KEY=sk-admin-change-this-key

